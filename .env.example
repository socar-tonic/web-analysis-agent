# Internal AI Router (highest priority, ChatOpenAI interface)
# INTERNAL_AI_URL=https://ai-router.company.com/v1
# INTERNAL_AI_KEY=your-api-key
# INTERNAL_AI_MODEL=gpt-4

# LLM API Keys (fallback options)
# GOOGLE_API_KEY=...
# OPENAI_API_KEY=sk-...
# ANTHROPIC_API_KEY=sk-ant-...

# LangSmith (optional, for tracing)
# LANGSMITH_API_KEY=lsv2_...
# LANGSMITH_PROJECT=web-analysis-agent
# LANGSMITH_TRACING=true

# Per-agent LLM configuration (optional)
# Tier: fast | balanced | powerful
# AGENT_LOGIN_TIER=balanced
# AGENT_SEARCH_TIER=fast
# Or specify exact model:
# AGENT_LOGIN_MODEL=claude-sonnet-4-20250514
# AGENT_SEARCH_MODEL=gemini-2.0-flash-lite

# Slack (optional, use 'mock' for console output)
SLACK_WEBHOOK_URL=mock

# Server
PORT=3000
